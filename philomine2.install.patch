+# =====================================================================
+# Subroutine: DiffRelativeRate
+# =====================================================================
+sub DiffRelativeRate {
+
+	my $debug = 0;
+
+	my $params  = $_[0];
+
+	my $runname = $params->{'runname'};
+	my $rundir =  $params->{'rundir'};
+
+	# The directory for this mining function for this run
+	my $function_dir = "$rundir/PerlDecisionTree";
