+		}
+        $divocauthor = $thisopenclosedata{"signed"};
+	delete $thisopenclosedata{"signed"};
+	if ($divocauthor) {
+		$divocauthor = &fixopenclose($divocauthor);
+		}
+	if (!$divocauthor) {
+        	$divocauthor = $thisopenclosedata{"author"};
+		delete $thisopenclosedata{"author"};
+		if ($divocauthor) {
+			$divocauthor = &fixopenclose($divocauthor);
+			}
