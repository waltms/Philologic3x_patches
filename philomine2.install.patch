+	$line =~ tr/\x{1F68}-\x{1F6F}/\x{1F60}-\x{1F67}/; ## omegas
+	$line =~ tr/\x{1FA8}-\x{1FAF}/\x{1FA0}-\x{1FA7}/;
+	$line =~ tr/\x{038F}\x{2126}\x{1FFA}-\x{1FFC}/\x{03CE}\x{03C9}\x{1FF2}\x{1FF4}\x{1FF3}/;
