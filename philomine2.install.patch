+################################################################################
+# scaleVector - multiply every value in the vector by a value
+################################################################################
+sub scaleVector() {
+	my $vector = $_[0];
+	my $scalar = $_[1];
+	foreach $feature (keys(%$vector)) {
+#		print $vector->{$feature};
+		$vector->{$feature} = $vector->{$feature} * $scalar;
+#		print "\n$vector->{$feature}";
+	}
+}
