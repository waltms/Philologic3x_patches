+	$line =~ s/\xe1\xbf\xac/\xcf\x81/gi;
+	
+	$line = Encode::decode("utf8", $line);	
+	$line =~ tr/\x{1F08}-\x{1F0F}/\x{1F00}-\x{1F07}/; ## alphas
+	$line =~ tr/\x{1F88}-\x{1F8F}/\x{1F80}-\x{1F87}/;
