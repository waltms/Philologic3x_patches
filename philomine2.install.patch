+# all of the current GET parameters.
+################################################################################
+sub getHiddenQSFields() {
+
+	my $params = $_[0];
+	
+	my $output = '';
+
+	foreach my $var (keys(%$params)) {
+		my $value = $params->{$var};
+		# Escape double quotes and backslashes
+		$value =~ s/\\/\\\\/g;
+		$value =~ s/"/\"/g;
+		$value =~ s/\+/ /g;
+		$output .= '<input type="hidden" name="' . $var . '" value="' . $value . "\">\n";		
+	}
+
+	return $output;
