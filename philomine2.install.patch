+sub JoinHyphenWords() {
+       $thewholething =~ s/(\&shy;[\n \t]*<lb\/>)/"_" x length($1)/gie;
+       $thewholething =~ s/(\&shy;[\n \t]*)/"_" x length($1)/gie;
