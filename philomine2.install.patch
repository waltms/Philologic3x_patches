+	$function_select .= '<option value="">Compiled</option>';
+	while ((my $internal, my $human) = each %compiled_functions) {
+		my $select = '';
+		if ($params->{'MINEFUN'} eq $internal) {
+			$select = " selected";
+		}
+		$function_select .= "<option value=\"$internal\"$select>&nbsp;&nbsp;&nbsp;$human</option>";
