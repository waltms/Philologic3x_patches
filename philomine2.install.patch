+################################################################################
+# getVectorLength - return the length of the vector
+################################################################################
+sub getVectorLength() {
+	my $vector = $_[0];
+	my $length = 0;
+	foreach my $value (values(%$vector)) {
+		$length += $value**2;
+	}
+ 	return sqrt($length);
+}
