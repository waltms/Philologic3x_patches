+	}
+	$AllRawChunks .= "\nSUBDIV\t";
+	$t = $docid . ":" . $DIV1 . ":" . $DIV2 . ":" . $DIV3 . ":" . $PARA;
+	$AllRawChunks .= $t . "\t";
+	$laststartid = $startid;
+	$lastendid = $endid;
+	}
+	
+     
