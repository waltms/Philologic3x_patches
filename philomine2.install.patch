+				} else {
+					$selected_string = '';
+				}
+
+				$output .= "<option value=\"$task\"$selected_string>" . $task_names{$task} . "</option>\n";
+			
+			}
+			$output .= '</select>';
+		}
+	}
+	
+	return $output;
+}
