+     $theword =~ s/\&#215;/\xc3\x97/g; # MULTIPLICATION SIGN
+     $theword =~ s/\&Oslash;/\xc3\x98/g;
+     $theword =~ s/\&Ugrave;/\xc3\x99/g;
+     $theword =~ s/\&Uacute;/\xc3\x9A/g;
