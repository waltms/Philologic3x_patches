+	
+		# URL encode the values, code taken from:
+		# http://support.internetconnection.net/CODE_LIBRARY/Perl_URL_Encode_and_Decode.shtml	
+		$value =~ s/([^A-Za-z0-9\-\.])/sprintf("%%%02X", ord($1))/seg;
+
+		$qs .= $var . '=' . $value . '&';
+	}
+	
+	$link = $PHILOMINECGIFORMURL . '?' . $qs . '&LINKEDFORM=yes';
+	return $link;
+}
